@model IEnumerable<MedicinaRegenerativa.Models.TipoTurnos>

@{
    ViewBag.Title = "Tipo Tunos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Listado Tipo de Turnos</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaCarga)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaCarga)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idTipoTurno }) |
            @Html.ActionLink("Detalle", "Details", new { id=item.idTipoTurno }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.idTipoTurno })
        </td>
    </tr>
}

</table>
