@model MedicinaRegenerativa.Models.PresupuestosIngresos

@{
    ViewBag.Title = "Borrar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Borrar</h2>

<h3>Esta seguro que quiere borrar?</h3>
<div>
    <h4>Ingresos en Presupuestos</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Monto)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Monto)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Cuota)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Cuota)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Observaciones)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Observaciones)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.FacturaNro)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FacturaNro)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.FechaCarga)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FechaCarga)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.MediosPago.Descripcion)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MediosPago.Descripcion)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Presupuestos.Observaciones)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Presupuestos.Observaciones)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.TiposMoneda.Descripcion)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TiposMoneda.Descripcion)
        </dd>

        <dt>
            Usuario que dio de alta
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AspNetUsers.Email)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Borrar" class="btn btn-default" /> |
            @Html.ActionLink("Volver", "Details", "Presupuestos", new { id = Model.idPresupuestos }, new { @class = "btn btn-primary" })
        </div>
    }
</div>
